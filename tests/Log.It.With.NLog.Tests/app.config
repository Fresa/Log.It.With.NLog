<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
   <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <extensions>
      <add assembly="Log.It.With.NLog.Tests" />
      <add assembly="Log.It.With.NLog" />
    </extensions>
    <variable name="Layout" value="item1=${lc:key=item1}, item2=${lc:key=item2}" />
    <targets>
      <target name="TestWriter" xsi:type="Test" layout="${Layout}" />
      <target name="Debugger" xsi:type="Debugger" layout="${Layout}" />
      <target name="XUnit2" xsi:type="XUnit2" layout="${Layout}" />
    </targets>
    <rules>
      <logger name="*" minLevel="Trace" writeTo="TestWriter,Debugger,XUnit2" />
    </rules>
  </nlog>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.0.0" newVersion="3.1.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>