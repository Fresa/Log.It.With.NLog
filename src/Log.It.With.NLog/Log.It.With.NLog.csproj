<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Authors>Fredrik Arvidsson</Authors>
    <Company />
    <Copyright>Copyright ©  2017</Copyright>
    <Description>A simple logging framework for NLog</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/Fresa/Log.It.With.NLog/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Fresa/Log.It.With.NLog</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Fresa/Log.It.With.NLog</RepositoryUrl>
    <Version>1.0.0</Version>
    <PackageVersion></PackageVersion>
    <PackageReleaseNotes />
    <PackageTags>log logging nlog</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Log.It" Version="3.0.1" />
    <PackageReference Include="NLog" Version="4.5.0" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-beta-63102-01" PrivateAssets="All" />
  </ItemGroup>
  <PropertyGroup Condition="'$(NCrunch)' == '1'">
    <EnableSourceControlManagerQueries>false</EnableSourceControlManagerQueries>
  </PropertyGroup>
</Project>